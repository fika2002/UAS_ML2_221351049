<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package= "com.example.flightticketpriceprediction">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Flight Price Predictor"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Material3.DayNight.NoActionBar"
        >

        <activity
            android:name=".SplashScreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".MainActivity" />
        <activity android:name=".TentangActivity" />
        <activity android:name=".FiturActivity" />
        <activity android:name=".ArsitekturActivity" />
        <activity android:name=".SimulasiActivity" />

    </application>
</manifest>